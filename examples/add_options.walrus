enum Option {
    None {},
    Some {x: Int},
}

fn add_options(a: Option, b: Option) -> Option {
    match (a, b) {
        (Option::Some{x:a}, Option::Some{x:b}) => Option::Some{x: a + b},
        _ => Option::None{},
    }
}

fn main() -> _ {
    let a = Option::Some{x:1};
    let b = Option::Some{x:2};
    add_options(a,b)
}
